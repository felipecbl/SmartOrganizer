{
    "saveAndRun": {
        "commands": [
            {
                "match": ".*\\.scss$",
                "cmd": "node-sass  \"${fileDirname}\" -o \"${fileDirname}/../\" --source-map true --output-style compressed -q ",
                "useShortcut": false,
                "silent": false
            }
        ]
    }
}